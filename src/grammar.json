{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "multipart",
  "rules": {
    "source_file": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "REPEAT1",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "delimiter_line"
                  },
                  {
                    "type": "REPEAT",
                    "content": {
                      "type": "SYMBOL",
                      "name": "header"
                    }
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_newline"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "body"
                  }
                ]
              }
            },
            {
              "type": "SYMBOL",
              "name": "delimiter_line"
            }
          ]
        },
        {
          "type": "BLANK"
        }
      ]
    },
    "delimiter_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "delimiter"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "delimiter": {
      "type": "PATTERN",
      "value": "--.+"
    },
    "header": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header_name"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "_whitespace"
        },
        {
          "type": "SYMBOL",
          "name": "header_value"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "header_name": {
      "type": "PATTERN",
      "value": "[A-Za-z0-9-_]+"
    },
    "header_value": {
      "type": "PATTERN",
      "value": ".*"
    },
    "body": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_data"
          },
          {
            "type": "SYMBOL",
            "name": "_newline"
          }
        ]
      }
    },
    "_whitespace": {
      "type": "PATTERN",
      "value": "\\s+"
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    },
    "_data": {
      "type": "PATTERN",
      "value": ".+"
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}